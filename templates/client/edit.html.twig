{% extends 'base.html.twig' %}

{% block title %}

{% endblock %}

{% block body %}
    <h3>Modification du client {{ client.reference }}</h3>
    <form action="" method="post" enctype="multipart/form-data">
        {{ form_widget(form._token) }}

        <div class="row">
            <div class="form-group col-md-6">
                {{ form_row(form.reference)}}
            </div>
        </div>

            <div class="form-group col-md-6">
                {{ form_row(form.email)}}
            </div>
            <div class="form-group col-md-6">
                {{ form_row(form.tel)}}
            </div>
        <div class="form-group mt-2 ">
            <button class="btn btn-primary " type="submit" name="submit" id="submit"><i class="fa fa-plane"></i>Modifier</button>
        </div>
    </form>
{% endblock %}

